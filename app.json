{
  "expo": {
    "name": "Quiz biblico",
    "slug": "quiz",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/bible.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/bible.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bible.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dhmes.quiz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ea2a5a1c-34df-4e4d-a99f-92ee4d709a4f"
      }
    },
    "owner": "dhmes"
  }
}
